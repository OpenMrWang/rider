  "body": [
    {
      "from": 0.26,
      "to": 2.68,
      "sid": 1,
      "location": 2,
      "content": "35岁网瘾中年假装自律的一天",
      "music": 0
    },
    {
      "from": 2.68,
      "to": 4.24,
      "sid": 2,
      "location": 2,
      "content": "自律第11天",
      "music": 0
    },
    {
      "from": 4.24,
      "to": 6.439,
      "sid": 3,
      "location": 2,
      "content": "今天开始加码",
      "music": 0
    },
    {
      "from": 12.05,
      "to": 13.45,
      "sid": 4,
      "location": 2,
      "content": "♪ Put it in a spin ♪",
      "music": 0.999999545454752
    },
    {
      "from": 13.45,
      "to": 14.81,
      "sid": 5,
      "location": 2,
      "content": "Like a minui",
      "music": 0
    },
    {
      "from": 14.81,
      "to": 15.47,
      "sid": 6,
      "location": 2,
      "content": "Aa good",
      "music": 0
    },
    {
      "from": 15.47,
      "to": 16.41,
      "sid": 7,
      "location": 2,
      "content": "And for it",
      "music": 0
    },
    {
      "from": 16.41,
      "to": 18.18,
      "sid": 8,
      "location": 2,
      "content": "No better know",
      "music": 0
    },
    {
      "from": 21.22,
      "to": 22.7,
      "sid": 9,
      "location": 2,
      "content": "戴上小头盔",
      "music": 0
    },
    {
      "from": 22.7,
      "to": 24.86,
      "sid": 10,
      "location": 2,
      "content": "我们今天骑车30km",
      "music": 0
    },
    {
      "from": 24.86,
      "to": 28.14,
      "sid": 11,
      "location": 2,
      "content": "去上海展览中心看一个展",
      "music": 0
    },
    {
      "from": 29.12,
      "to": 31.98,
      "sid": 12,
      "location": 2,
      "content": "Get get get get get get get",
      "music": 0
    },
    {
      "from": 31.98,
      "to": 33.72,
      "sid": 13,
      "location": 2,
      "content": "早上灌的一壶冰咖啡",
      "music": 0
    },
    {
      "from": 33.72,
      "to": 35.91,
      "sid": 14,
      "location": 2,
      "content": "现在已经变成了热咖啡",
      "music": 0
    },
    {
      "from": 36.39,
      "to": 37.83,
      "sid": 15,
      "location": 2,
      "content": "♪ 放过这一次 ♪",
      "music": 0.9999999107142936
    },
    {
      "from": 37.83,
      "to": 38.71,
      "sid": 16,
      "location": 2,
      "content": "♪ 下辈子 ♪",
      "music": 0.9999998750000156
    },
    {
      "from": 38.71,
      "to": 42.39,
      "sid": 17,
      "location": 2,
      "content": "我想这是我从我长大以来第一次做百渡过江",
      "music": 0
    },
    {
      "from": 42.39,
      "to": 43.47,
      "sid": 18,
      "location": 2,
      "content": "去浦西",
      "music": 0
    },
    {
      "from": 43.47,
      "to": 45.09,
      "sid": 19,
      "location": 2,
      "content": "♪ 不会有太多心事 ♪",
      "music": 0.9999999259259313
    },
    {
      "from": 45.09,
      "to": 46.13,
      "sid": 20,
      "location": 2,
      "content": "♪ 用最简单 ♪",
      "music": 0.9137929458977604
    },
    {
      "from": 46.13,
      "to": 50.12,
      "sid": 21,
      "location": 2,
      "content": "刚买还是象象征性的锁一下余生",
      "music": 0
    },
    {
      "from": 51.92,
      "to": 55.64,
      "sid": 22,
      "location": 2,
      "content": "第一天骑行骑了18km",
      "music": 0
    },
    {
      "from": 55.64,
      "to": 58,
      "sid": 23,
      "location": 2,
      "content": "用了两个小时",
      "music": 0
    },
    {
      "from": 58,
      "to": 60.92,
      "sid": 24,
      "location": 2,
      "content": "有没有什么可以夹在车上的包推荐啊",
      "music": 0
    },
    {
      "from": 60.92,
      "to": 63.18,
      "sid": 25,
      "location": 2,
      "content": "我带了这样一个包就好麻烦啊",
      "music": 0
    },
    {
      "from": 63.18,
      "to": 65.12,
      "sid": 26,
      "location": 2,
      "content": "主要是我要带充电宝",
      "music": 0
    },
    {
      "from": 65.12,
      "to": 66,
      "sid": 27,
      "location": 2,
      "content": "兄弟们",
      "music": 0
    },
    {
      "from": 67.04,
      "to": 68.18,
      "sid": 28,
      "location": 2,
      "content": "我人傻了",
      "music": 0
    },
    {
      "from": 68.18,
      "to": 70.789,
      "sid": 29,
      "location": 2,
      "content": "站台摊位都在搭着",
      "music": 0
    },
    {
      "from": 70.789,
      "to": 72.189,
      "sid": 30,
      "location": 2,
      "content": "他是13号开始",
      "music": 0
    },
    {
      "from": 72.189,
      "to": 76.069,
      "sid": 31,
      "location": 2,
      "content": "也就是说我骑了18km来这个上海书展",
      "music": 0
    },
    {
      "from": 76.069,
      "to": 78.2,
      "sid": 32,
      "location": 2,
      "content": "然后他没开门",
      "music": 0
    },
    {
      "from": 78.2,
      "to": 81.86,
      "sid": 33,
      "location": 2,
      "content": "之前每天早上起来感觉走1万步要死要活的",
      "music": 0
    },
    {
      "from": 81.86,
      "to": 83.18,
      "sid": 34,
      "location": 2,
      "content": "但现在骑了车以后",
      "music": 0
    },
    {
      "from": 83.18,
      "to": 84.9,
      "sid": 35,
      "location": 2,
      "content": "感觉走路是一种放松",
      "music": 0
    },
    {
      "from": 84.9,
      "to": 87.16,
      "sid": 36,
      "location": 2,
      "content": "你把自己面对的困难加大",
      "music": 0
    },
    {
      "from": 87.16,
      "to": 89.96,
      "sid": 37,
      "location": 2,
      "content": "然后你以前的困难他就变得不是困难",
      "music": 0
    },
    {
      "from": 89.96,
      "to": 91.14,
      "sid": 38,
      "location": 2,
      "content": "现在5000步",
      "music": 0
    },
    {
      "from": 91.14,
      "to": 94.01,
      "sid": 39,
      "location": 2,
      "content": "现在这走1万步就跟散步一样",
      "music": 0
    },
    {
      "from": 96.62,
      "to": 98.76,
      "sid": 40,
      "location": 2,
      "content": "嗯脉动没有嘉德乐好喝",
      "music": 0
    },
    {
      "from": 98.76,
      "to": 100.5,
      "sid": 41,
      "location": 2,
      "content": "买了一个这个玩意",
      "music": 0
    },
    {
      "from": 100.5,
      "to": 101.52,
      "sid": 42,
      "location": 2,
      "content": "夹车版的",
      "music": 0
    },
    {
      "from": 101.52,
      "to": 102.97,
      "sid": 43,
      "location": 2,
      "content": "二百五",
      "music": 0
    },
    {
      "from": 102.97,
      "to": 105.89,
      "sid": 44,
      "location": 2,
      "content": "1万步完成回家",
      "music": 0
    },
    {
      "from": 105.89,
      "to": 106.21,
      "sid": 45,
      "location": 2,
      "content": "哎",
      "music": 0
    },
    {
      "from": 106.21,
      "to": 107.95,
      "sid": 46,
      "location": 2,
      "content": "不对的生活",
      "music": 0.27692305562130054
    },
    {
      "from": 107.95,
      "to": 112.26,
      "sid": 47,
      "location": 2,
      "content": "我车呢找车的过程又多走了2000步啊",
      "music": 0
    },
    {
      "from": 112.26,
      "to": 114.02,
      "sid": 48,
      "location": 2,
      "content": "有时候明明很累",
      "music": 0.7230768674556254
    },
    {
      "from": 114.02,
      "to": 117.91,
      "sid": 49,
      "location": 2,
      "content": "却不知道为什么要坚持山区那积水",
      "music": 0.5466666423703732
    },
    {
      "from": 117.91,
      "to": 120.65,
      "sid": 50,
      "location": 2,
      "content": "♪ 美神坚强的城市擦干眼泪 ♪",
      "music": 0.9999999596774211
    },
    {
      "from": 120.65,
      "to": 125.21,
      "sid": 51,
      "location": 2,
      "content": "在这机灵得碎的日子一笑了之",
      "music": 0.777777749900438
    },
    {
      "from": 126.54,
      "to": 129.8,
      "sid": 52,
      "location": 2,
      "content": "呼我的屁股疼",
      "music": 0
    },
    {
      "from": 129.8,
      "to": 131.42,
      "sid": 53,
      "location": 2,
      "content": "磨完疼",
      "music": 0
    },
    {
      "from": 131.42,
      "to": 132.98,
      "sid": 54,
      "location": 2,
      "content": "脖子疼",
      "music": 0
    },
    {
      "from": 132.98,
      "to": 134.34,
      "sid": 55,
      "location": 2,
      "content": "手疼",
      "music": 0
    },
    {
      "from": 134.34,
      "to": 136.04,
      "sid": 56,
      "location": 2,
      "content": "肩膀疼",
      "music": 0
    },
    {
      "from": 136.42,
      "to": 138.02,
      "sid": 57,
      "location": 2,
      "content": "肘子抬",
      "music": 0
    },
    {
      "from": 138.41,
      "to": 143.6,
      "sid": 58,
      "location": 2,
      "content": "我要给我以前对于骑行的这项运动的小康道歉",
      "music": 0
    },
    {
      "from": 143.6,
      "to": 145.36,
      "sid": 59,
      "location": 2,
      "content": "我今天一共骑了40km",
      "music": 0
    },
    {
      "from": 145.36,
      "to": 147.52,
      "sid": 60,
      "location": 2,
      "content": "最后的20km骑了两个半小时",
      "music": 0
    },
    {
      "from": 147.52,
      "to": 150.17,
      "sid": 61,
      "location": 2,
      "content": "你们那些说我运动量不够的兄弟们",
      "music": 0
    },
    {
      "from": 150.17,
      "to": 151.79,
      "sid": 62,
      "location": 2,
      "content": "今天给你们看看够不够",
      "music": 0
    },
    {
      "from": 151.79,
      "to": 153.29,
      "sid": 63,
      "location": 2,
      "content": "现在是三点",
      "music": 0
    },
    {
      "from": 153.29,
      "to": 156.13,
      "sid": 64,
      "location": 2,
      "content": "我今天的运动量是九千三百六十千卡",
      "music": 0
    },
    {
      "from": 156.13,
      "to": 158.3,
      "sid": 65,
      "location": 2,
      "content": "你要不要称一下现在自己体重多少",
      "music": 0
    },
    {
      "from": 158.3,
      "to": 159.34,
      "sid": 66,
      "location": 2,
      "content": "我很好奇啊",
      "music": 0
    },
    {
      "from": 159.34,
      "to": 160.7,
      "sid": 67,
      "location": 2,
      "content": "就我觉得应该不会很瘦",
      "music": 0
    },
    {
      "from": 160.7,
      "to": 162.92,
      "sid": 68,
      "location": 2,
      "content": "因为我今天一路上喝了五瓶水",
      "music": 0
    },
    {
      "from": 162.92,
      "to": 165.18,
      "sid": 69,
      "location": 2,
      "content": "然后我一次都没上过厕所",
      "music": 0
    },
    {
      "from": 166.03,
      "to": 169.13,
      "sid": 70,
      "location": 2,
      "content": "卧槽这个是有点夸张了吧",
      "music": 0
    },
    {
      "from": 169.13,
      "to": 172.17,
      "sid": 71,
      "location": 2,
      "content": "我刚刚坐在那边缓了大概半个多小时",
      "music": 0
    },
    {
      "from": 172.17,
      "to": 173.13,
      "sid": 72,
      "location": 2,
      "content": "就麻的感觉",
      "music": 0
    },
    {
      "from": 173.13,
      "to": 175.86,
      "sid": 73,
      "location": 2,
      "content": "从脚下慢慢升上来升上来",
      "music": 0
    },
    {
      "from": 175.86,
      "to": 178.58,
      "sid": 74,
      "location": 2,
      "content": "然后我就突然感到一阵撕心裂肺的恶",
      "music": 0
    },
    {
      "from": 178.58,
      "to": 178.9,
      "sid": 75,
      "location": 2,
      "content": "水",
      "music": 0
    },
    {
      "from": 178.9,
      "to": 179.64,
      "sid": 76,
      "location": 2,
      "content": "煮鸡胸肉",
      "music": 0
    },
    {
      "from": 179.64,
      "to": 181.48,
      "sid": 77,
      "location": 2,
      "content": "水煮蔬菜",
      "music": 0
    },
    {
      "from": 181.48,
      "to": 184.99,
      "sid": 78,
      "location": 2,
      "content": "然后一点点杂粮米饭",
      "music": 0
    },
    {
      "from": 184.99,
      "to": 187.11,
      "sid": 79,
      "location": 2,
      "content": "这是我今天的中饭和晚饭",
      "music": 0
    },
    {
      "from": 187.11,
      "to": 188.17,
      "sid": 80,
      "location": 2,
      "content": "今天吃晚了",
      "music": 0
    },
    {
      "from": 188.17,
      "to": 190.62,
      "sid": 81,
      "location": 2,
      "content": "因为路上花了太久",
      "music": 0
    },
    {
      "from": 190.98,
      "to": 193.38,
      "sid": 82,
      "location": 2,
      "content": "♪ 谁不是奔跑在黑暗中追寻 ♪",
      "music": 0.9999999107142936
    },
    {
      "from": 193.38,
      "to": 195.51,
      "sid": 83,
      "location": 2,
      "content": "我不知道明天我还能不能动啊",
      "music": 0
    },
    {
      "from": 195.51,
      "to": 197.55,
      "sid": 84,
      "location": 2,
      "content": "因为我还是想去那个上海书展",
      "music": 0
    },
    {
      "from": 197.55,
      "to": 198.95,
      "sid": 85,
      "location": 2,
      "content": "所以我明天其实很想",
      "music": 0
    },
    {
      "from": 198.95,
      "to": 202.89,
      "sid": 86,
      "location": 2,
      "content": "就沿着今天的完全一模一样的路线再走一次",
      "music": 0
    },
    {
      "from": 202.89,
      "to": 205.999,
      "sid": 87,
      "location": 2,
      "content": "今天这接近40km",
      "music": 0
    },
    {
      "from": 205.999,
      "to": 208.739,
      "sid": 88,
      "location": 2,
      "content": "我花了五个小时的孩子",
      "music": 0.26732671943926606
    },
    {
      "from": 208.739,
      "to": 209.799,
      "sid": 89,
      "location": 2,
      "content": "♪ 有人激浪 ♪",
      "music": 0.9999998911860837
    },
    {
      "from": 209.799,
      "to": 211.05,
      "sid": 90,
      "location": 2,
      "content": "♪ 有人依靠不回 ♪",
      "music": 0.9999999020568165
    },
    {
      "from": 211.05,
      "to": 211.85,
      "sid": 91,
      "location": 2,
      "content": "我跟你们说",
      "music": 0
    },
    {
      "from": 211.85,
      "to": 213.03,
      "sid": 92,
      "location": 2,
      "content": "就骑完车回来",
      "music": 0
    },
    {
      "from": 213.03,
      "to": 213.99,
      "sid": 93,
      "location": 2,
      "content": "啥都没干",
      "music": 0
    },
    {
      "from": 213.99,
      "to": 215.67,
      "sid": 94,
      "location": 2,
      "content": "但我觉得打斗还是必须的",
      "music": 0
    },
    {
      "from": 215.67,
      "to": 219.89,
      "sid": 95,
      "location": 2,
      "content": "因为动和静还是要同时修炼",
      "music": 0
    },
    {
      "from": 219.89,
      "to": 221.35,
      "sid": 96,
      "location": 2,
      "content": "有时候明明很累",
      "music": 0.7807016859033582
    },
    {
      "from": 221.35,
      "to": 224.57,
      "sid": 97,
      "location": 2,
      "content": "♪ 却不知道为什么要坚持装酷的现实 ♪",
      "music": 0.9999999664429542
    },
    {
      "from": 224.57,
      "to": 225.21,
      "sid": 98,
      "location": 2,
      "content": "♪ 在积水 ♪",
      "music": 0.9999998437500245
    },
    {
      "from": 225.21,
      "to": 228.34,
      "sid": 99,
      "location": 2,
      "content": "内心坚强的生实在是疼的不行了",
      "music": 0.4456928672025156
    },
    {
      "from": 228.34,
      "to": 230.63,
      "sid": 100,
      "location": 2,
      "content": "做个运动康复吧",
      "music": 0
    },
    {
      "from": 230.79,
      "to": 236.07,
      "sid": 101,
      "location": 2,
      "content": "哇哦哦哦哦哦哦哦哦哦那那个搓麻筋的感觉",
      "music": 0.0917431171618496
    },
    {
      "from": 236.07,
      "to": 239.24,
      "sid": 102,
      "location": 2,
      "content": "哦哦呃呃呃呃呃",
      "music": 0.09999999374999861
    },
    {
      "from": 239.24,
      "to": 241.16,
      "sid": 103,
      "location": 2,
      "content": "这个不是头不行不行",
      "music": 0
    },
    {
      "from": 241.16,
      "to": 243.85,
      "sid": 104,
      "location": 2,
      "content": "不要说话不要说话啊",
      "music": 0
    },
    {
      "from": 243.85,
      "to": 245.25,
      "sid": 105,
      "location": 2,
      "content": "对对好",
      "music": 0.19298243921206323
    },
    {
      "from": 245.25,
      "to": 247.23,
      "sid": 106,
      "location": 2,
      "content": "保持住保持住卧蚕",
      "music": 0
    },
    {
      "from": 247.23,
      "to": 248.77,
      "sid": 107,
      "location": 2,
      "content": "这这太可了啊",
      "music": 0
    },
    {
      "from": 248.77,
      "to": 254.24,
      "sid": 108,
      "location": 2,
      "content": "到哎哎哎哎哎哎哎哎哥们狗啊",
      "music": 0.2869565092627611
    },
    {
      "from": 254.24,
      "to": 256.54,
      "sid": 109,
      "location": 2,
      "content": "怪不居然是这个衣服",
      "music": 0.4814814517604035
    },
    {
      "from": 261.13,
      "to": 265.6,
      "sid": 110,
      "location": 2,
      "content": "啊哈哈哈",
      "music": 0.14285713605442985
    },
    {
      "from": 266.66,
      "to": 269.04,
      "sid": 111,
      "location": 2,
      "content": "就像是做了一场运动一样",
      "music": 0
    },
