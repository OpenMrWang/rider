    },
    {
      "from": 200.17,
      "to": 202.75,
      "sid": 72,
      "location": 2,
      "content": "每个汉堡大概都要排半个小时以上",
      "music": 0
    },
    {
      "from": 202.75,
      "to": 204.13,
      "sid": 73,
      "location": 2,
      "content": "好夸张啊",
      "music": 0
    },
    {
      "from": 204.13,
      "to": 205.17,
      "sid": 74,
      "location": 2,
      "content": "这一次",
      "music": 0.999999545454752
    },
    {
      "from": 205.17,
      "to": 208.65,
      "sid": 75,
      "location": 2,
      "content": "然后现在我要先休息一会",
      "music": 0
    },
    {
      "from": 208.65,
      "to": 210.99,
      "sid": 76,
      "location": 2,
      "content": "然后既然我已经来到静安了嘛",
